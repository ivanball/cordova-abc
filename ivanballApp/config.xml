<?xml version="1.0" encoding="utf-8" standalone="no"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="io.cordova.myappe32ade" version="1.1.2">
  <name>ivanballApp</name>
  <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
  <content src="index.html" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <plugin name="cordova-plugin-battery-status" version="1.1.1" />
  <plugin name="cordova-plugin-camera" version="2.1.0" />
  <plugin name="cordova-plugin-media-capture" version="1.2.0" />
  <plugin name="cordova-plugin-console" version="1.0.2" />
  <plugin name="cordova-plugin-contacts" version="2.0.1" />
  <plugin name="cordova-plugin-device" version="1.1.1" />
  <plugin name="cordova-plugin-device-motion" version="1.2.0" />
  <plugin name="cordova-plugin-device-orientation" version="1.0.2" />
  <plugin name="cordova-plugin-file-transfer" version="1.5.0" />
  <plugin name="cordova-plugin-geolocation" version="2.1.0" />
  <plugin name="cordova-plugin-globalization" version="1.0.2" />
  <plugin name="cordova-plugin-media" version="2.1.0" />
  <plugin name="cordova-plugin-network-information" version="1.2.0" />
  <plugin name="cordova-plugin-dialogs" version="1.2.0" />
  <plugin name="cordova-plugin-splashscreen" version="3.1.0" />
  <plugin name="cordova-plugin-vibration" version="2.1.0" />
  <plugin name="cordova-plugin-whitelist" version="1.2.1" />
</widget>